{
  "manifest_version": 2,

  "minimum_chrome_version": "9",

  "name": "Whatsup SF!",

  "description": "This extension displays popular events around your area",

  "version": "1.0",

  "omnibox":{
    "keyword": "whatsup sf"
  },

  "background":{
    "scripts":["background.js"]
  },

  "browser_action": {
    "default_icon": "event.png",
    "default_popup": "popup.html",
    "default_title": "Check out events!"
  },

  "permissions": [
    "background",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage"
  ],

  "content_security_policy": "script-src 'self'"

}